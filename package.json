{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.node.json --noEmit",
    "dev": "vite --port 5175",
    "build": "vite build",
    "lint": "eslint .",
    "ok": "npm run format:check && npm run type-check && npm run lint && npm run build && npm test --if-present",
    "preview": "vite preview",
    "up": "npx -y npm-check-updates@latest -u && npm i",
    "knip": "knip --production --fix --allow-remove-files --format --exports"
  },
  "dependencies": {
    "@artifact/client": "npm:@jsr/artifact__client@^0.0.161",
    "@dreamcatcher/chats": "npm:@jsr/dreamcatcher__chats@^0.0.14",
    "@llm-ui/code": "^0.13.3",
    "@llm-ui/markdown": "^0.13.3",
    "@llm-ui/react": "^0.13.3",
    "@open-iframe-resizer/react": "^1.6.0",
    "@privy-io/react-auth": "2.18.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "debug": "^4.4.1",
    "delay": "^6.0.0",
    "fast-deep-equal": "^3.1.3",
    "html-react-parser": "^5.2.5",
    "immer": "^10.1.1",
    "jest-diff": "^30.0.4",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.8.0",
    "use-stick-to-bottom": "^1.1.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsr": "^0.13.5",
    "knip": "^5.61.3",
    "ox": "^0.8.4",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^7.0.4"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "trailingComma": "none"
  },
  "overrides": {
    "@llm-ui/react": {
      "react": "^19.1"
    },
    "@llm-ui/markdown": {
      "react": "^19.1"
    },
    "@llm-ui/code": {
      "react": "^19.1",
      "shiki": "^3.8.0"
    }
  }
}
